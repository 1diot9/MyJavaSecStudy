    <Manager className="org.apache.catalina.session.PersistentManager" maxIdleSwap="1">
        <Store className="org.apache.catalina.session.FileStore" directory="D:/sec_software/environment/apache-tomcat-9.0.48/work" />
    </Manager>
</Context>